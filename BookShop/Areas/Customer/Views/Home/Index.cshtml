@using BookShop.Models;
@model List<Products>
@{
    ViewData["Title"] = "Home Page";
}

<br /><br />

<div class="row">
    @foreach (var book in Model)
    {
        <div class="col-4">
            <div class="card mb-4">
                <div class="card-header">
                    <h4 class="my-0 font-weight-normal">
                        <label style="font-size: 23px;">@book.Name</label>
                    </h4>
                </div>
            </div>
        </div>
    }
</div>